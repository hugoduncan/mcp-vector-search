{:paths ["src"]
 :deps  {io.github.hugoduncan/mcp-clj-server
         {:git/url   "https://github.com/hugoduncan/mcp-clj"
          :git/sha   "50a2d1ab100c27b76e80b5674d6c2ee567b079b6"
          :deps/root "projects/server"}

         dev.langchain4j/langchain4j
         {:mvn/version "1.4.0"}

         dev.langchain4j/langchain4j-core
         {:mvn/version "1.4.0"}

         dev.langchain4j/langchain4j-embeddings-all-minilm-l6-v2
         {:mvn/version "1.4.0-beta10"
          :exclusions  [ai.djl.huggingface/tokenizers]}
         ai.djl.huggingface/tokenizers {:mvn/version "0.28.0"}

         org.slf4j/slf4j-nop   {:mvn/version "2.0.17"}
         org.clojure/clojure   {:mvn/version "1.12.3"}
         org.clojure/data.json {:mvn/version "2.5.1"}
         hawk/hawk             {:mvn/version "0.2.11"}
         io.github.classgraph/classgraph {:mvn/version "4.8.181"}}

 :aliases
 {:dev {:extra-paths ["test"]
        :extra-deps  {babashka/fs         {:mvn/version "0.5.23"}}
        :override-deps {org.hugpduncan/mcp-clj-server
                        {:git/url   "https://github.com/hugoduncan/mcp-clj"
                         :git/sha   "50a2d1ab100c27b76e80b5674d6c2ee567b079b6"
                         :deps/root "projects/test-dep"}}
        :jvm-opts    ["--enable-native-access=ALL-UNNAMED"]}
  :test {:extra-paths ["test"]
         :extra-deps  {lambdaisland/kaocha {:mvn/version "1.91.1392"}
                       babashka/fs         {:mvn/version "0.5.23"}}
         :override-deps {org.hugpduncan/mcp-clj-server
                         {:git/url   "https://github.com/hugoduncan/mcp-clj"
                          :git/sha   "50a2d1ab100c27b76e80b5674d6c2ee567b079b6"
                          :deps/root "projects/test-dep"}}
         :jvm-opts    ["--enable-native-access=ALL-UNNAMED"]
         :main-opts   ["-m" "kaocha.runner"]}
  :nrepl {:extra-deps {nrepl/nrepl {:mvn/version "1.3.1"}}
          :main-opts ["-m" "nrepl.cmdline"]}
  :run  {:exec-fn  mcp-vector-search.main/start
         :main-opts   ["-m" "mcp-vector-search.main"]
         :jvm-opts ["--enable-native-access=ALL-UNNAMED"]}}}
